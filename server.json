{
    "name":"wheelie",
    "force":true,
    "web":{
        "http":{
            "enable":true,
            "port":"8080"
        },
        "rewrites":{
            "enable":true,
            "config":"urlrewrite.xml"
        },
        "ssl":{
            "enable":true,
            "port":"8443"
        },
        "host":"0.0.0.0"
    },
    "app":{
        "cfengine":"lucee@5",
        "serverHomeDirectory":"/root/serverHome"
    },
    "openbrowser":"false",
    "JVM":{
        "args":"-Dcom.sun.net.ssl.enableECC=false",
        "minHeapSize":"2048",
        "heapSize":"2048"
    }
}
