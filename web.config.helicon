<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <httpErrors errorMode="Detailed" />
    <security>
		<requestFiltering>
			<requestLimits maxAllowedContentLength="999999999" />
		</requestFiltering>
	</security>
   </system.webServer>
	<system.web>
		<httpRuntime maxRequestLength="999999" executionTimeout="999999" /> 
	</system.web>
</configuration>




